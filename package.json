{
  "name": "ayi-project",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "backend": "yarn workspace backend dev",
    "frontend": "yarn workspace @ayi/frontend dev",
    "start": "yarn workspaces foreach -pi run dev",
    "clean": "rimraf '**/node_modules' '**/dist' '.yarn' 'yarn.lock'",
    "reset": "yarn clean && yarn cache clean && yarn install",
    "tree": "treee -l -p -a",
    "build": "yarn workspaces foreach --all run build"
  },
  "dependencies": {
    "ayi-project": "file:",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.49.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "rimraf": "^5.0.1"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "yarn@4.7.0",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0"
  }
}
